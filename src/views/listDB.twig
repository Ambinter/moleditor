{% if dbs %}
      <li class="dropdown">
        <a href="#" class="dropdown-toggle" data-toggle="dropdown">My Databases <b class="caret"></b></a>
        <ul class="dropdown-menu">
	    {% for db in dbs %}
		    {% if db.key  %}
			<li><a href="{{ path('display', {'key':db.key}) }}">{{db.dbname}}</a></li>
		    {% endif %}
	    {% endfor %}
        </ul>
      </li>
{% endif %}
